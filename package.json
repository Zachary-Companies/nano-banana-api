{
  "name": "@zachary/nano-banana-api",
  "version": "1.0.9",
  "description": "Image generation client using Gemini native image generation (Nano Banana)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "publish:patch": "npm version patch && npm publish",
    "all": "npm run clean && npm run build && npm test && npm run commit && npm run publish:patch",
    "commit": "git add . && git commit -m \"Auto-update\" || true",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@google/genai": "^1.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
